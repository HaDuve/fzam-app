<?xml version="1.0" encoding="utf-8"?>

<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="FZAM.Pages.LightTheme">

   <!--Purple-->
   <Color x:Key="FimPurple">#764a6b</Color>
   <!--Graph Colors-->
   <Color x:Key="GraphBlue">#207de5</Color>
   <Color x:Key="GraphRed">#e11d21</Color>
   <Color x:Key="GraphPurple">#764a6b</Color>

   <!--Page Titles-->
    <Style x:Key="PageTitleLabel" TargetType="Label">
        <Setter Property="VerticalOptions" Value="Start"/>
        <Setter Property="FontAttributes" Value="Bold"/>
        <Setter Property="TextColor" Value="{StaticResource FimPurple}"/>
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="MaxLines" Value="1"/>
    </Style>




</ResourceDictionary>